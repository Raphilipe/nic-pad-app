<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:sf="sap.f" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" controllerName="sap.ui.demo.todo.controller.User" displayBlock="false">
	<Page title="{i18n>userTitle}" showNavButton="true" navButtonPress=".onNavBack">
		<headerContent>
			<Button id="buttonChangePassword" text="{i18n>buttonChangePassword}" press="onChangePassword" icon="sap-icon://edit" type="Transparent"></Button>
		</headerContent>
		<content>
			<VBox class="sapUiSmallMargin">
				<f:SimpleForm id="SimpleFormDisplay471" editable="false" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="4" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="1" emptySpanL="1" emptySpanM="1" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false">
					<f:content>
						<Label id="labelUser" text="{i18n>labelUser}" labelFor="textUser" />
						<Text id="textUser" text="{userModel>/user}" />
						<Label id="labelFirstName" text="{i18n>labelFirstName}" labelFor="textFirstName" />
						<Text id="textFirstName" text="{userModel>/firstName}" />
						<Label id="labelLastName" text="{i18n>labelLastName}" labelFor="textLastName" />
						<Text id="textLastName" text="{userModel>/lastName}" />
					</f:content>
				</f:SimpleForm>
			</VBox>
		</content>
	</Page>
</mvc:View>